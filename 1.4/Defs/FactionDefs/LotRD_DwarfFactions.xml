<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <FactionDef ParentName="FactionBase">
    <defName>LotRD_MountainKingdom</defName>
    <label>kingdom under the mountain</label>
    <fixedName>Kingdom Under the Mountain</fixedName>
    <description>.</description>
    <pawnSingular>mountain dwarf</pawnSingular>
    <pawnsPlural>mountain dwarves</pawnsPlural>
    <!-- <categoryTag> -->
    <!-- <listOrderPriority> -->
    <settlementGenerationWeight>0.2</settlementGenerationWeight>
    <requiredCountAtGameStart>1</requiredCountAtGameStart>
    <maxConfigurableAtWorldCreation>9999</maxConfigurableAtWorldCreation>
    <canMakeRandomly>true</canMakeRandomly>
    <canSiege>true</canSiege>
    <canStageAttacks>true</canStageAttacks>
    <leaderTitle>king</leaderTitle>
    <factionIconPath>World/MountainDwarves</factionIconPath>
    <techLevel>Medieval</techLevel>
    <!-- <factionNameMaker> -->
    <settlementNameMaker>LotRD_NamerFactionBaseDwarfMountain</settlementNameMaker>
    <allowedCultures><li>Corunan</li></allowedCultures>
    <requiredMemes>
      <li MayRequire="Ludeon.RimWorld.Ideology">Loyalist</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Structure_TheistAbstract</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Tunneler</li> <!-- Mountain Dwarves -->
    </requiredMemes>
    <allowedMemes>
      <li MayRequire="Ludeon.RimWorld.Ideology">Loyalist</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Structure_TheistAbstract</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Tunneler</li> <!-- Mountain Dwarves -->
      <li MayRequire="Ludeon.RimWorld.Ideology">Darkness</li> <!-- Mountain Dwarves -->    
      <li MayRequire="Ludeon.RimWorld.Ideology">Collectivist</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">FleshPurity</li>
    </allowedMemes>
    <structureMemeWeights>
      <Structure_TheistAbstract MayRequire="Ludeon.RimWorld.Ideology">1</Structure_TheistAbstract>
    </structureMemeWeights>
    <backstoryCategories><li>LotR_BackstoryDwarf</li></backstoryCategories>
    <caravanTraderKinds>
      <li>LotRD_Caravan_Dwarf_BulkGoods</li>
      <li>LotRD_Caravan_Dwarf_CombatSupplier</li>
    </caravanTraderKinds>
    <visitorTraderKinds>
      <li>Visitor_Outlander_Standard</li>
    </visitorTraderKinds>
    <baseTraderKinds>
      <li>Base_Outlander_Standard</li>
    </baseTraderKinds>
    <allowedArrivalTemperatureRange>
      <min>-40</min>
      <max>45</max>
    </allowedArrivalTemperatureRange>
    <maxPawnCostPerTotalPointsCurve>
      <points>
        <li>(0,35)</li>
        <li>(70, 50)</li>
        <li>(700, 100)</li>
        <li>(1300, 150)</li>
        <li>(100000, 10000)</li>
      </points>
    </maxPawnCostPerTotalPointsCurve>
    <pawnGroupMakers>
      <li>
        <kindDef>Peaceful</kindDef>
        <options>
          <LotRD_DwarfGuardMountain>100</LotRD_DwarfGuardMountain>
          <LotRD_DwarfVillager>100</LotRD_DwarfVillager>
          <!--
            TODO: BIOTECH 
            <LotRD_DwarfVillagerChild MayRequire="Ludeon.RimWorld.Biotech">20</LotRD_DwarfVillagerChild>
          -->
          <LotRD_DwarfSoldierMountain>20</LotRD_DwarfSoldierMountain>
        </options>
      </li>
      <li>
        <kindDef>Combat</kindDef>
        <options>
          <LotRD_DwarfGuardMountain>100</LotRD_DwarfGuardMountain>
          <LotRD_DwarfSoldierMountain>20</LotRD_DwarfSoldierMountain>
          <LotRD_DwarfKing>5</LotRD_DwarfKing>
        </options>
      </li>
      <li>
        <kindDef>Trader</kindDef>
        <traders>
          <LotRD_DwarfTrader>1</LotRD_DwarfTrader>
        </traders>
        <carriers>
          <Muffalo>6</Muffalo>
        </carriers>
        <guards>
          <LotRD_DwarfVillager>50</LotRD_DwarfVillager>
          <LotRD_DwarfGuardMountain>100</LotRD_DwarfGuardMountain>
        </guards>
      </li>
      <li>
        <kindDef>Settlement</kindDef>
        <options>
          <LotRD_DwarfVillager>50</LotRD_DwarfVillager>
          <LotRD_DwarfGuardMountain>100</LotRD_DwarfGuardMountain>
          <LotRD_DwarfSoldierMountain>20</LotRD_DwarfSoldierMountain>
          <LotRD_DwarfKing>10</LotRD_DwarfKing>
        </options>
      </li>
    </pawnGroupMakers>
    <settlementTexturePath>World/WorldObjects/DefaultSettlement</settlementTexturePath>
    <!-- <modExtensions> -->
    <!-- <li Class="JecsTools.FactionSettings"> -->
    <!-- <entrySoundDef>LotRD_FanfareDwarfMountain</entrySoundDef> -->
    <!-- </li> -->
    <!-- </modExtensions> -->
  </FactionDef>
