  <ThingDef ParentName="ArtableFurnitureBase">
    <defName>LotRD_DwarvenThrone</defName>
    <label>dwarven throne</label>
    <description>The seat of nobility for dwarf kind.</description>
    <size>(3,3)</size>
    <graphicData>
      <texPath>Things/Building/Furniture/sheidulaEreborThrone</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(8,8)</drawSize>
      <damageData>
        <rect>(0.05,0.1,0.9,0.9)</rect>
      </damageData>
      <drawRotated>false</drawRotated>
    </graphicData>
    <defaultPlacingRot>South</defaultPlacingRot>
    <altitudeLayer>Item</altitudeLayer>
    <category>Building</category>
    <thingSetMakerTags>
      <li>RewardStandardQualitySuper</li>
    </thingSetMakerTags>
    <statBases>
      <MaxHitPoints>400</MaxHitPoints>
      <WorkToBuild>2200</WorkToBuild>
      <Mass>300</Mass>
      <Flammability>0.0</Flammability>
      <Beauty>20</Beauty>
      <Comfort>1.2</Comfort>
    </statBases>
    <passability>Standable</passability>
    <costList>
      <Gold>10</Gold>
    </costList>
    <stuffCategories>
      <li>Metallic</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>300</costStuffCount>
    <pathCost>80</pathCost>
    <rotatable>false</rotatable>
    <fillPercent>0.75</fillPercent>
    <building>
      <isSittable>true</isSittable>
    </building>
    <placeWorkers>
      <li>JecsTools.PlaceWorker_Outline</li>
    </placeWorkers>
    <constructionSkillPrerequisite>6</constructionSkillPrerequisite>
    <researchPrerequisites>
