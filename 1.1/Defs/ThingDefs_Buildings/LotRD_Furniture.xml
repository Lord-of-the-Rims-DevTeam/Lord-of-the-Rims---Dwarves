<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="FurnitureWithQualityBase">
    <defName>LotRD_GameTable</defName>
    <label>dwarven game table</label>
    <description>A table designed for playing a variety of stone throwing games. A great way to test one's strategic skills and a bit of luck.</description>
    <thingCategories Inherit="False">
      <li>LotRD_BuildingsDwarven</li>
    </thingCategories>
    <graphicData>
      <texPath>Things/Building/Joy/edmundWulfgarthDwarvenGameTable</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(2,2)</drawSize>
    </graphicData>
    <staticSunShadowHeight>0.20</staticSunShadowHeight>
    <altitudeLayer>Building</altitudeLayer>
    <socialPropernessMatters>true</socialPropernessMatters>
    <fillPercent>0.40</fillPercent>
    <building>
      <joyKind>Gaming_Cerebral</joyKind>
    </building>
    <statBases>
      <MaxHitPoints>250</MaxHitPoints>
      <WorkToBuild>2100</WorkToBuild>
      <Mass>120</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>0</Beauty>
      <JoyGainFactor>1</JoyGainFactor>
    </statBases>
    <size>(1,1)</size>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>80</costStuffCount>
    <costList>
      <Cloth>80</Cloth>
      <WoodLog>30</WoodLog>
    </costList>
    <pathCost>70</pathCost>
    <passability>PassThroughOnly</passability>
    <designationCategory>Joy</designationCategory>
    <designationHotKey>Misc5</designationHotKey>
    <canOverlapZones>false</canOverlapZones>
    <researchPrerequisites>
      <li>LotRD_FurnitureOne</li>
    </researchPrerequisites>
  </ThingDef>
  <ThingDef ParentName="ArtableFurnitureBase">
    <defName>LotRD_Dresser</defName>
    <label>dwarven dresser</label>
    <description>Gives a small comfort bonus to all nearby beds in dwarven style. Placing more than one dresser near the same bed will have no effect.</description>
    <graphicData>
      <texPath>Things/Building/Furniture/edmundWulfgarthDwarvenDresser</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(2.9,2.9)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.4</fillPercent>
    <castEdgeShadows>true</castEdgeShadows>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
    <defaultPlacingRot>South</defaultPlacingRot>
    <costStuffCount>60</costStuffCount>
    <statBases>
      <MaxHitPoints>115</MaxHitPoints>
      <Mass>16</Mass>
      <WorkToBuild>2100</WorkToBuild>
      <Flammability>1.0</Flammability>
      <Beauty>5</Beauty>
    </statBases>
    <size>(2,1)</size>
    <designationHotKey>Misc12</designationHotKey>
    <staticSunShadowHeight>0.5</staticSunShadowHeight>
    <surfaceType>Item</surfaceType>
    <canOverlapZones>false</canOverlapZones>
    <placeWorkers>
      <li>PlaceWorker_ShowFacilitiesConnections</li>
    </placeWorkers>
    <comps>
      <li Class="CompProperties_Facility">
        <statOffsets>
          <Comfort>0.05</Comfort>
        </statOffsets>
        <maxSimultaneous>1</maxSimultaneous>
        <maxDistance>6</maxDistance>
      </li>
    </comps>
    <researchPrerequisites>
      <li>LotRD_FurnitureOne</li>
    </researchPrerequisites>
  </ThingDef>
  <ThingDef ParentName="TableBase">
    <defName>LotRD_EndTable</defName>
    <label>dwarven end table</label>
    <description>A small dwarven bedside table. Makes the nearby beds a little bit more comfortable. Must be placed directly adjacent to their head. Placing more than one end table near the same bed will have no effect.</description>
    <surfaceType>Item</surfaceType>
    <graphicData>
      <texPath>Things/Building/Furniture/edmundWulfgarthDwarvenEndTable</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shadowData>
        <volume>(0.3, 0.6, 0.3)</volume>
      </shadowData>
      <drawSize>1.2</drawSize>
    </graphicData>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToBuild>1250</WorkToBuild>
      <Mass>7</Mass>
      <Flammability>0.8</Flammability>
      <Beauty>5</Beauty>
    </statBases>
    <defaultPlacingRot>South</defaultPlacingRot>
    <costStuffCount>40</costStuffCount>
    <placeWorkers>
      <li>PlaceWorker_ShowFacilitiesConnections</li>
    </placeWorkers>
    <comps>
      <li Class="CompProperties_Facility">
        <statOffsets>
          <Comfort>0.05</Comfort>
        </statOffsets>
        <maxSimultaneous>1</maxSimultaneous>
        <mustBePlacedAdjacentCardinalToBedHead>true</mustBePlacedAdjacentCardinalToBedHead>
      </li>
    </comps>
    <staticSunShadowHeight>0</staticSunShadowHeight>
    <castEdgeShadows>false</castEdgeShadows>
    <rotatable>true</rotatable>
    <researchPrerequisites>
      <li>LotRD_FurnitureOne</li>
    </researchPrerequisites>
  </ThingDef>
