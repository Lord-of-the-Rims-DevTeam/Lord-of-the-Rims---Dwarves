  <ThingDef ParentName="ArtableFurnitureBase">
    <defName>LotRD_Stool</defName>
    <label>dwarven stool</label>
    <graphicData>
      <texPath>Things/Building/Furniture/edmundWulfgarthDwarvenStool</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>1.15</drawSize>
      <shadowData>
        <volume>(0.3, 0.6, 0.3)</volume>
      </shadowData>
      <damageData>
        <rectN>(0.125,0.296875,0.734375,0.421875)</rectN>
        <rectS>(0.125,0.296875,0.734375,0.421875)</rectS>
        <rectE>(0.234375,0.296875,0.5,0.546875)</rectE>
        <rectW>(0.234375,0.296875,0.5,0.546875)</rectW>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToBuild>1050</WorkToBuild>
      <Mass>3</Mass>
      <Flammability>0.8</Flammability>
      <Beauty>1.2</Beauty>
      <Comfort>0.65</Comfort>
    </statBases>
    <description>A simple platform on which to sit fashioned by the dwarves.</description>
    <socialPropernessMatters>true</socialPropernessMatters>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>30</costStuffCount>
